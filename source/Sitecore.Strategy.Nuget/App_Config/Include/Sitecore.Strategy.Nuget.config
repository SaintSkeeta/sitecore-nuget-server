                    <param desc="id">$(id)</param>
                    <isMinInclusive>true</isMinInclusive>
                </dependency>
                
              </dependencies>
          </package>
          <package id="Sitecore.ExperienceEditor.CoreGroup" type="Sitecore.Strategy.Nuget.Configuration.PackageDescriptor, Sitecore.Strategy.Nuget">
